{
    "name": "sierratecnologia/transmissor",
    "description": "Simple user messaging and notifications tool for Laravel",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "transmissor",
        "laravel", "messenger", "messaging", "user messaging"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/transmissor/",
    "support": {
        "email": "help@sierratecnologia.com.br",
        "issues": "https://github.com/sierratecnologia/transmissor/issues",
        "source": "https://github.com/sierratecnologia/transmissor",
        "docs": "https://github.com/sierratecnologia/transmissor/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/config": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0",
        "sierratecnologia/muleta": "dev-master",
        "sierratecnologia/crypto": "dev-master",
        "sierratecnologia/integrations": "dev-master",
        "sierratecnologia/porteiro": "dev-master",
        "sierratecnologia/population": "dev-master",
        "sierratecnologia/locaravel": "dev-master",
        "sierratecnologia/telefonica": "dev-master",
        "ramsey/uuid": ">=3.8",
        "guzzlehttp/guzzle": "^6.0 || ^7.0 || ^8.0",
        "cmgmyr/messenger": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "barryvdh/laravel-ide-helper": ">=2.7",
        "codedungeon/phpunit-result-printer": ">=0.27.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0",
        "adamwathan/faktory": "0.3.*",
        "friendsofphp/php-cs-fixer": "^2.18",
        "orchestra/testbench": "^3.0|^4.0|^5.0|^6.0"
    },
    "suggest": {
        "fzaninotto/faker": "*"
    },
    "autoload": {
        "psr-4": {
            "Transmissor\\": "src/"
        },
        "files": [
            "helpers/activity_helper.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html build/coverage",
        "format": "vendor/bin/php-cs-fixer fix",
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [{"type": "path","url":"../**","options":{"symlink": true}}],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Transmissor\\TransmissorProvider"
            ]
        }
    }
}
